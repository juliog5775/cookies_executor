{
  "manifest_version": 3,
  "name": "Cookies Executor",
  "version": "1.0",
  "description": "__MSG_extension_description__",
  "default_locale":"en",
  "permissions": ["storage","cookies","tabs","activeTab"],
  "host_permissions":["*://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon":{ 
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  }
    
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  
}
